# MYSQL_HOST=127.0.0.1
# MYSQL_PORT=3306
# MYSQL_USER=root
# MYSQL_PASSWORD=
# MYSQL_NAME=wordpress

# WP table prefix
MYSQL_PREFIX="wp_"

