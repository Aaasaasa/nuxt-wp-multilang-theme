generator client {
  provider = "prisma-client-js"
  output   = "./generated/mysql"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model WpPost {
  ID          Int      @id @default(autoincrement())
  post_title  String
  post_name   String
  post_date   DateTime
  post_type   String
  post_status String
}
